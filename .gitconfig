[user]
	name = Tim Wingfield
  email = tim@timwingfield.com
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[merge]
	tool = opendiff
  keepBackup = false
[mergetool "p4merge"]
  cmd = p4merge "$BASE" "$LOCAL" "$REMOTE" "$MERGED"
  keepTemporaries = false
  trustExitCode = false
  keepBackup = false
[diff]
  tool = p4diff
	external = /Users/Timbo/bin/git-diff-cmd.sh
[difftool "p4diff"]
  prompt = false
  cmd = p4merge "$LOCAL" "$BASE"
[core]
  autocrlf = input
  legacyheaders = false
	editor = vim
[repack]
  usedeltabaseoffset = true
[alias]
  ci = commit
  co = checkout
  st = status
  br = branch
  ch = cherry -v origin
  mine = log --author=Tim
  spull = !git svn fetch && git svn rebase
  spush = !git svn dcommit
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[github]
  user = timwingfield
  token = d3578984cad0594ff05530bb188061be
[credential]
	helper = osxkeychain
[push]
	default = current
